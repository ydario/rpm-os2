    (('a', '=', '1.2'),		('a', '<>', '1.3'),	1),

    # Simple, obvious version-release comparisons
    (('a', '=', '1.2-1'),	('a', '=', '1.2-1'),	1),
    (('a', '=', '1.2-1'),	('a', '>=', '1.2-1'),	1),
    (('a', '=', '1.2-1'),	('a', '<=', '1.2-1'),	1),
