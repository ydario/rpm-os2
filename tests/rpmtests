    (('a', '<=', '1.2'),	('a', '>', '1.2-1'),	1),
    (('a', '<=', '1.2'),	('a', '<>', '1.2-1'),	1),

    (('a', '<>', '1.2'),	('a', '=', '1.2-1'),	0),
    (('a', '<>', '1.2'),	('a', '>=', '1.2-1'),	1),
    (('a', '<>', '1.2'),	('a', '<=', '1.2-1'),	1),
    (('a', '<>', '1.2'),	('a', '<', '1.2-1'),	1),
    (('a', '<>', '1.2'),	('a', '>', '1.2-1'),	1),
    (('a', '<>', '1.2'),	('a', '<>', '1.2-1'),	1),
]

ms = ['no match', 'match']

myprint() # dumb kludge to fixup expected output
for p, r, res in tests:
    d1 = rpm.ds(p, 'provides')
    d2 = rpm.ds(r, 'requires')
    match = d1.Compare(d2)
    if match != res:
        myprint('FAILED: %s with %s: %s' % (d1.DNEVR(), d2.DNEVR(),ms[res]))

EOF
python test.py

) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/rpmdepmatch.at:4"
$at_failed && at_fn_log_failure
$at_traceon; }


  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
read at_status <"$at_status_file"
#AT_STOP_287
